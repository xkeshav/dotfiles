{
  "dateTime.showMonth": true,
  "dateTime.showDayOfMonth": true,
  "dateTime.showAMPM": true,
  "dateTime.showDayOfWeek": true,
  "debug.javascript.codelens.npmScripts": "never",
  "debug.toolBarLocation": "floating",
  "diffEditor.ignoreTrimWhitespace": true,
  "editor.codeActionsOnSave": {
    "source.fixAll": true,
    "source.organizeImports": true
  },
  "editor.acceptSuggestionOnEnter": "off",
  "editor.accessibilitySupport": "off",
  "editor.comments.insertSpace": false,
  "editor.cursorBlinking": "solid",
  "editor.cursorStyle": "line",
  "editor.cursorWidth": 3,
  "editor.detectIndentation": true,
  "editor.find.autoFindInSelection": true,
  "editor.find.globalFindClipboard": true,
  "editor.find.seedSearchStringFromSelection": true,
  "editor.fontFamily": "Fira code, Operator Mono, Menlo, Monaco",
  "editor.fontLigatures": true,
  "editor.fontSize": 14,
  "editor.fontWeight": "400",
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.formatOnType": true,
  "editor.inlayHints.enabled": "onUnlessPressed",
  "editor.insertSpaces": false,
  "editor.letterSpacing": 0.5,
  "editor.lineHeight": 22,
  "editor.linkedEditing": true,
  "editor.minimap.enabled": false,
  "editor.minimap.renderCharacters": false,
  "editor.minimap.size": "fit",
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.renderWhitespace": "none",
  "editor.scrollBeyondLastLine": true,
  "editor.selectionHighlight": true,
  "editor.showFoldingControls": "always",
  "editor.snippetSuggestions": "top",
  "editor.suggestSelection": "first",
  "editor.tabCompletion": false,
  "editor.tabSize": 2,
  "editor.wordWrap": "on",
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "explorer.compactFolders": false,
  "explorer.excludeGitIgnore": true,
  "explorer.fileNesting.expand": false,
  "explorer.fileNesting.enabled": true,
  "explorer.fileNesting.patterns": {
    "*.ts": "${capture}.js",
    "*.js": "${capture}.js.map, ${capture}.min.js, ${capture}.d.ts",
    "*.jsx": "${capture}.js",
    "*.tsx": "${capture}.ts",
    "tsconfig.json": "tsconfig.*.json",
    "package.json": "package-lock.json, yarn.lock",
    ".gitignore": ".eslint*,.editorconfig,.npm*,.babelrc,.prettier*,.git*"
  },
  "extensions.ignoreRecommendations": true,
  "extensions.autoUpdate": "onlyEnabledExtensions",
  "extensions.showRecommendationsOnlyOnDemand": true,
  "files.hotExit": "onExit",
  "files.encoding": "utf8",
  "files.defaultLanguage": "markdown",
  "files.autoSave": "afterDelay",
  "files.autoSaveDelay": 1000,
  "files.trimTrailingWhitespace": true,
  "git.enableSmartCommit": true,
  "git.autofetch": true,
  "git.postCommitCommand": "push",
  "git.rememberPostCommitCommand": true,
  "git.branchProtection": ["master", "develop"],
  "git.branchPrefix": "zeroPoint/",
  "git.branchRandomName.dictionary": ["numbers"],
  "javascript.preferences.quoteStyle": "single",
  "open-in-browser.default": "google-chrome-stable",
  "prettier.printWidth": 140,
  "prettier.singleQuote": true,
  "prettier.tabWidth": 2,
  "prettier.eslintIntegration": true,
  "projectManager.git.baseFolders": ["/Users/keshavmohta/Developer"],
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[html]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "vscode.html-language-features",
    "editor.quickSuggestions": {
      "other": true,
      "comments": false,
      "strings": true
    }
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.formatOnSave": true,
    "editor.formatOnPaste": true
  },
  "[typescript]": {
    "editor.formatOnSave": true,
    "editor.formatOnPaste": true,
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "[markdown]": {
    "editor.formatOnSave": true,
    "editor.renderWhitespace": "all",
    "editor.wordWrap": "on",
    "editor.quickSuggestions": false
  },
  "files.exclude": {
    "**/.git": true,
    "**/.DS_Store": true,
    "**/node_modules/": true
  },
  "search.exclude": {
    "node_modules/": true,
    "bower_components/": true,
    "e2e/": true,
    "dist": true
  },
  "emmet.includeLanguages": {
    "javascript": "javascriptreact"
  },
  "settingsSync.ignoredSettings": ["workbench.colorTheme"],
  "screencastMode.onlyKeyboardShortcuts": true,
  "search.showLineNumbers": true,
  "security.workspace.trust.untrustedFiles": "open",
  "typescript.format.insertSpaceAfterOpeningAndBeforeClosingJsxExpressionBraces": true,
  "typescript.preferences.importModuleSpecifier": "relative",
  "typescript.preferences.quoteStyle": "single",
  "typescript.showUnused": true,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "terminal.explorerKind": "external",
  "terminal.integrated.copyOnSelection": true,
  "terminal.integrated.defaultProfile.windows": "Command Prompt",
  "vsicons.dontShowNewVersionMessage": true,
  "workbench.editorAssociations": {
    "*.log": "default"
  },
  "workbench.activityBar.visible": true,
  "workbench.colorCustomizations": {
    "editor.selectionBackground": "#b4afaf",
    "editor.selectionHighlightBackground": "#d9ebaa",
    "statusBar.background": "#33101a",
    "statusBar.noFolderBackground": "#0A0A0D",
    "statusBar.debuggingBackground": "#511f1f"
  },
  "workbench.colorTheme": "Light Pink",
  "workbench.editor.openSideBySideDirection": "down",
  "workbench.editor.showTabs": true,
  "workbench.iconTheme": "seti",
  "workbench.layoutControl.enabled": false,
  "workbench.layoutControl.type": "menu",
  "workbench.list.multiSelectModifier": "alt",
  "workbench.sideBar.location": "left",
  "workbench.startupEditor": "newUntitledFile",
  "window.autoDetectColorScheme": true,
  "window.closeWhenEmpty": true,
  "window.openFilesInNewWindow": "off",
  "window.title": "${activeFolderMedium}${separator}${activeEditorShort}${separator}${rootName}${separator}${dirty}",
  "window.titleBarStyle": "custom",
  "window.zoomLevel": 0,
  "cSpell.userWords": ["Keshav", "Mohta", "xkeshav"],
  "cSpell.customDictionaries": {
    "myDictionary": {
      "name": "myDictionary",
      "path": "~/myDictionary.txt",
      "scope": "user",
      "addWords": true
    }
  },
  "cSpell.enableFiletypes": [
